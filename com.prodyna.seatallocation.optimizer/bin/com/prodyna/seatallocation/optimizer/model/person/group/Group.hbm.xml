<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.prodyna.seatallocation.optimizer.model.person.group">
	<class name="Group">
		<id name="id" type="long">
			<generator class="native" />
		</id>
		
		<property name="name" type="String" not-null="true" length="40" />
		
	</class>
</hibernate-mapping>